  "homepage": "https://github.com/cam-inc/dmc#readme",
  "scripts": {
    "start": "npm run watch",
    "watch": "run-p watch:js watch:css",
    "watch:js": "rollup -c --watch",
    "watch:css": "watch 'npm run build:css' s/css",
    "build": "run-s build:clean build:prepare build:js build:css",
    "build:clean": "echo TODO",
    "build:prepare": "echo TODO",
    "build:js": "echo TODO",
    "build:css": "run-s build:css:bundle build:css:minify",
    "build:css:bundle": "postcss s/css/app.css -o dist/css/app.css",
    "build:css:minify": "csswring dist/css/app.css dist/css/app.min.css",
    "test": "run-s format lint spec",
    "format": "run-p format:js format:css",
    "format:js": "echo TODO",
    "format:css": "stylefmt -c .stylefmtrc -r s/css/**/*",
    "lint": "run-p lint:js lint:css",
    "lint:js": "eslint s",
    "lint:css": "stylelint s/css/**/*.css",
    "spec": "echo TODO",
    "ncu": "ncu -a"
  },
  "dependencies": {
    "history": "^4.6.1",
    "marked": ">=0.3.6",
    "mout": "^1.0.0",
    "object-assign": "^4.1.1",
    "path-to-regexp": "^1.7.0",
    "riot": ">=3.5.0",
    "store": "=1.3.20",
    "suitcss-base": "^2.0.0",
    "taucharts": "^1.1.3",
    "url-polyfill": "git+https://github.com/webcomponents/URL.git",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "browserslist": "^1.7.7",
    "csswring": "^5.1.1",
    "npm-check-updates": ">=2.8.8",
    "npm-run-all": "^4.0.2",
    "postcss-apply": "^0.6.1",
    "postcss-cli": "^3.1.1",
    "postcss-currentcolor": "0.0.2",
    "postcss-custom-properties": "^5.0.2",
    "postcss-flexbugs-fixes": "^2.1.1",
    "postcss-import": "^9.1.0",
    "postcss-nesting": "^2.3.1",
    "postcss-overflow-wrap": "0.0.1",
    "postcss-sorting": "^2.0.1",
    "pug": ">=2.0.0-beta11",
    "rollup": ">=0.36.4",
    "rollup-plugin-buble": ">=0.14.0",
    "rollup-plugin-commonjs": ">=5.0.5",
    "rollup-plugin-eslint": ">=3.0.0",
    "rollup-plugin-filesize": ">=1.0.1",
    "rollup-plugin-json": ">=2.1.0",
    "rollup-plugin-node-resolve": ">=2.0.0",
    "rollup-plugin-replace": ">=1.1.1",
    "rollup-plugin-riot": ">=1.0.0",
    "rollup-plugin-serve": ">=0.1.0",
    "rollup-plugin-uglify": ">=1.0.1",
    "rollup-watch": ">=2.5.0",
    "stylefmt": "^5.3.2",
    "stylelint": "^7.10.1",
    "stylelint-suitcss": "^1.0.0",
    "watch": "^1.0.2"
  },
