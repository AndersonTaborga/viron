{
  "name": "dmc",
  "version": "0.0.1",
  "description": "Design-based Management Console.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cam-inc/dmc.git"
  },
  "keywords": [
    "management",
    "console",
    "admin",
    "design",
    "swagger",
